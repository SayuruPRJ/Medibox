    {
        "id": "ac2457fce61aa5c3",
        "type": "function",
        "z": "93349f73afce91b1",
        "name": "function 1",
        "func": "\nif (msg.topic === \"sample_interval\") {\n    flow.set(\"sample_interval\", msg.payload);\n    // Create payload with only sample_interval\n    msg.payload = {\n        sample_interval: msg.payload\n    };\n} else if (msg.topic === \"sample_send_period\") {\n    // Convert minutes to seconds \n    let sample_send_period_in_seconds = msg.payload * 60;\n    flow.set(\"sample_send_period\", sample_send_period_in_seconds);\n    // Create payload with only sample_send_period\n    msg.payload = {\n        sample_send_period: sample_send_period_in_seconds\n    };\n} else if (msg.topic === \"theta_offset\") {\n    flow.set(\"theta_offset\", msg.payload);\n    // Create payload with only theta_offset\n    msg.payload = {\n        theta_offset: msg.payload\n    };\n} else if (msg.topic === \"t_med\") {\n    flow.set(\"t_med\", msg.payload);\n    // Create payload with only t_med\n    msg.payload = {\n        t_med: msg.payload\n    };\n} else if (msg.topic === \"control_factor\") {\n    flow.set(\"control_factor\", msg.payload);\n    // Create payload with only control_factor\n    msg.payload = {\n        control_factor: msg.payload\n    };\n}\n\n// Return msg only if payload was set \nif (msg.payload) {\n    return msg;\n}",
        "outputs": 1,
        "timeout": 0,
